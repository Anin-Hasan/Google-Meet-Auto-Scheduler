{
  "manifest_version": 3,
  "name": "Auto Google Meet Joiner",
  "version": "1.0",
  "permissions": ["alarms", "tabs", "scripting", "storage"],
  "host_permissions": ["https://meet.google.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.jpg"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon.jpg",
    "48": "icons/icon.jpg",
    "128": "icons/icon.jpg"
  }
}
